<UserControl x:Class="ProjectWS.Editor.UI.Toolbox.TerrainPropPlacePane"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjectWS.Editor.UI.Toolbox"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="400">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="10,10" Foreground="WhiteSmoke" Name="textBlock_SelectedProp">Selected Prop</TextBlock>
            <TextBlock Margin="10,10" Foreground="WhiteSmoke" Name="textBlock_PropDebugDetails">Info</TextBlock>
        </StackPanel>
    </Grid>
</UserControl>
